{
  "name": "effective-transporter",
  "version": "0.1.0",
  "description": "Transporter utilities for safely sending non-POJO values between server and client in SvelteKit.",
  "type": "module",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "@sveltejs/kit": "^2.11.0",
    "effect": "^3.19.13"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.11.0",
    "effect": "^3.19.13"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "sveltekit",
    "transporter",
    "serialization",
    "effect",
    "effect-ts",
    "non-pojo"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jannabiforever/effective-transporter.git"
  }
}
